{
    "name": "ulrack/orm-extension",
    "description": "The ORM extension for Ulrack Services.",
    "keywords": [
        "services",
        "orm",
        "databases"
    ],
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "grizz-it/configuration": "^1.3",
        "grizz-it/dbal": "^1.0",
        "grizz-it/dbal-sql": "^1.0",
        "grizz-it/object-factory": "^1.2",
        "grizz-it/search": "^2.1",
        "grizz-it/services": "^1.0",
        "grizz-it/storage": "^1.1"
    },
    "authors": [
        {
            "name": "Ulrack",
            "homepage": "https://www.ulrack.com/",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ulrack\\OrmExtension\\": "src/"
        },
        "files": [
            "locator.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ulrack\\OrmExtension\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            "/.gitignore",
            "/.travis.yml",
            "/phpunit.xml",
            "/phpcs.xml",
            "/PULL_REQUEST_TEMPLATE.md",
            "/CODE_OF_CONDUCT.md",
            "/CONTRIBUTING.md"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
